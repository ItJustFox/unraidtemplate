  <Config Name="Configuration &amp;amp; Tiles" Target="/data/" Default="/mnt/user/appdata/TileServer-GL/" Mode="rw" Description="Config file for TileServer" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/TileServer-GL/</Config>
